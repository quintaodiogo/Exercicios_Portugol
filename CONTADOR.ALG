Algoritmo "contador"

Var
   contador,valor,salto:inteiro

Inicio
      contador <- 0
      Escreval("Quer contar até quanto ? ")
      Leia(valor)
      Escreval("Qual será o valor do salto ?")
      Leia(salto)
      Enquanto (contador<=valor) faca
               Escreval(contador)
               contador<-contador+salto
               FimEnquanto
               
Fimalgoritmo